{
  "name": "history-scraper",
  "version": "0.0.1",
  "description": "Scrapes the history of a website using the Wayback Machine provided a custom cawler is provided.",
  "private": true,
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --config nodemon.json --exec babel-node ./bin/www",
    "start-debug": "DEBUG=history-scraper:* yarn start",
    "test": "eslint bin/www routes/**/*.js app.js src && flow"
  },
  "keywords": [
    "scraper",
    "nodejs",
    "javascript",
    "history",
    "wayback",
    "machine",
    "wayback-machine",
    "waybackmachine"
  ],
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.3",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-beta11",
    "serve-favicon": "^2.4.2",
    "stylus": "^0.54.5"
  },
  "author": "Nakshatra Mukhopadhyay",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.43.1"
  }
}
