{
  "name": "history-scraper",
  "version": "0.0.1",
  "description": "Scrapes the history of a website using the Wayback Machine provided a custom cawler is provided.",
  "private": true,
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "start-debug": "DEBUG=history-scraper:* yarn dev:start",
    "dev:start": "nodemon --config nodemon.json --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js",
    "test": "yarn lint && flow && flow-coverage-report -i 'src/**/*.js' -t html && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "keywords": [
    "scraper",
    "nodejs",
    "javascript",
    "history",
    "wayback",
    "machine",
    "wayback-machine",
    "waybackmachine"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "common-tags": "^1.4.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.3",
    "express": "^4.15.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-beta11",
    "serve-favicon": "^2.4.2",
    "stylus": "^0.54.5"
  },
  "author": "Nakshatra Mukhopadhyay",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.43.1",
    "flow-coverage-report": "^0.3.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "pm2": "^2.4.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
